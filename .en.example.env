# This is an example environment file.
# Copy this file to .env and replace the placeholder values with your actual credentials.

# Cisco Firewall
CISCO_FIREWALL_API_TOKEN=your_cisco_firewall_api_token
CISCO_FIREWALL_HOST=your_cisco_firewall_host

# Cisco Secure Endpoint
CISCO_SECURE_ENDPOINT_API_KEY=your_cisco_secure_endpoint_api_key
CISCO_SECURE_ENDPOINT_BASE_URL=your_cisco_secure_endpoint_base_url

# Database
DB_HOST=your_db_host
DB_USER=your_db_user
DB_PASSWORD=your_db_password
DB_NAME=your_db_name

# Node.js API
# NODE_API_BASE_URL=http://localhost:3000

# Splunk MCP
SPLUNK_MCP_BEARER_TOKEN=your_splunk_mcp_bearer_token
SPLUNK_MCP_BASE_URL=your_splunk_mcp_base_url

# Splunk SOAR
SPLUNK_SOAR_API_KEY=your_splunk_soar_api_key
SPLUNK_SOAR_BASE_URL=your_splunk_soar_base_url

# Anthropic API Configuration
ANTHROPIC_API_KEY=your_anthropic_api_key